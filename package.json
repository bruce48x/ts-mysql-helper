{
    "name": "ts-mysql-helper",
    "version": "1.3.2",
    "description": "mysql helper",
    "main": "./dist/index.js",
    "keywords": [
        "mysql",
        "typescript"
    ],
    "author": "bruce48x",
    "license": "MIT",
    "dependencies": {
        "@types/lodash": "^4.14.130",
        "@types/mysql": "^2.15.6",
        "intelli-espower-loader": "^1.0.1",
        "lodash": "^4.17.11",
        "mysql2": "^2.3.3"
    },
    "devDependencies": {
        "@types/chai": "^4.1.7",
        "@types/jest": "^24.0.15",
        "@types/supertest": "^2.0.8",
        "jest": "^24.8.0",
        "supertest": "^4.0.2",
        "ts-jest": "^24.0.2",
        "ts-node": "^7.0.1",
        "typescript": "^2.9.2"
    },
    "scripts": {
        "build": "tsc",
        "testmocha": "mocha --require intelli-espower-loader --exit ./dist/test/*.js",
        "test": "jest --forceExit"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/bruce48x/ts-mysql-helper"
    },
    "jest": {
        "verbose": false,
        "moduleFileExtensions": [
            "js",
            "json",
            "ts",
            "node"
        ],
        "rootDir": ".",
        "testRegex": ".test.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
